{
  "name": "athena-agent",
  "version": "2.0.0",
  "description": "Intelligent Multi-Agent Orchestration Platform - Supercharge your AI Coding Agents",
  "main": "packages/core/dist/index.js",
  "types": "packages/core/dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/athena-agent.git"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn src.main:app --reload",
    "build": "npm run build:packages && npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:packages": "npm run build:types && npm run build:core && npm run build:sdk",
    "build:types": "cd packages/types && npm run build",
    "build:core": "cd packages/core && npm run build",
    "build:sdk": "cd packages/sdk && npm run build",
    "test": "npm run test:backend && npm run test:frontend && npm run test:packages",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm run test",
    "test:packages": "npm run test:core && npm run test:sdk",
    "test:core": "cd packages/core && npm run test",
    "test:sdk": "cd packages/sdk && npm run test",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install"
  },
  "workspaces": [
    "packages/*",
    "frontend"
  ],
  "keywords": [
    "ai",
    "agent",
    "orchestration",
    "multi-agent",
    "coding-assistant",
    "openclaw",
    "skills",
    "automation"
  ],
  "author": "Athena Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
